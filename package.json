{
  "name": "taro-unocss",
  "version": "0.0.0",
  "private": true,
  "description": "A Taro + Vue3 + Typescript template with unocss and auto import",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "none"
  },
  "author": "",
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "eslint ./**/*.{js,ts,vue}",
    "lint:fix": "npm run lint --fix",
    "pre:gen-taro-api": "npx tsx config/module/TaroResolver.ts"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.5.0-beta.2",
    "@tarojs/plugin-framework-vue3": "3.5.0-beta.2",
    "@tarojs/runtime": "3.5.0-beta.2",
    "@tarojs/taro": "3.5.0-beta.2",
    "@vueuse/core": "^8.6.0",
    "pinia": "^2.0.14",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@babel/core": "^7.8.0",
    "@iconify-json/carbon": "^1.1.5",
    "@iconify-json/mdi": "^1.1.19",
    "@tarojs/plugin-html": "^3.5.0-beta.2",
    "@tarojs/webpack5-runner": "3.5.0-beta.2",
    "@types/webpack-env": "^1.13.6",
    "@unocss/webpack": "^0.35.4",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.2.37",
    "babel-preset-taro": "3.5.0-beta.2",
    "eslint": "^8.16.0",
    "local-pkg": "^0.4.1",
    "stylelint": "^14.4.0",
    "typescript": "^4.1.0",
    "unocss": "^0.35.4",
    "unplugin-auto-import": "^0.8.7",
    "unplugin-vue-components": "^0.19.6",
    "vue-loader": "^17.0.0",
    "webpack": "^5.69.0"
  }
}
